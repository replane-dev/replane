  "scripts": {
    "dev": "next dev --turbopack | pino-pretty",
    "build": "next build && cp -r public .next/standalone/ && cp -r .next/static .next/standalone/.next/",
    "start": "NODE_ENV=production node .next/standalone/server.js",
    "start:inspect": "NODE_ENV=production node --inspect=0.0.0.0:9229 .next/standalone/server.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint",
    "test": "dotenv -- vitest run --exclude '**/e2e/**'",
    "test:watch": "dotenv -- vitest --exclude '**/e2e/**'",
    "test:e2e": "dotenv -- vitest run tests/e2e/",
    "bench": "dotenv -e bench/.env -- k6 run --out experimental-prometheus-rw bench/scripts/spec.ts",
    "soak": "TEST_DURATION=1h SSE_VUS=500 ADMIN_VUS=3 dotenv -e bench/.env -- k6 run --out experimental-prometheus-rw bench/scripts/spec.ts",
    "release": "bumpp",
    "migrate": "pnpm run migrate:migrate && pnpm run migrate:codegen",
    "migrate:migrate": "dotenv -- tsx ./src/engine/scripts/migrate.ts",
    "migrate:cleanup": "dotenv -- tsx ./src/engine/scripts/cleanup-database.ts",
    "migrate:codegen": "pnpm run migrate:cleanup && dotenv -- kysely-codegen --out-file ./src/engine/core/db.d.ts && prettier --write ./src/engine/core/db.d.ts && eslint ./src/engine/core/db.d.ts --fix"
  },
